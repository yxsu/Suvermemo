f\xe8\x83\xbd\xe7\x9a\x84\xe3\x80\x82\xe4\xb8\x94\xe5\xbe\x97\xe4\xb9\x8b\xe4\xba\x8e\xe6\xad\xa4\xe8\x80\x85\xef\xbc\x8c\xe5\x8f\x88\xe5\xa4\xb1\xe4\xb9\x8b\xe4\xba\x8e\xe5\xbd\xbc\xe3\x80\x82\xe4\xbe\x8b\xe5\xa6\x82\xef\xbc\x8c\xe5\xb0\x86\xe5\x8d\x8e\xe5\x8c\x97\xe5\x85\xb5\xe5\x8a\x9b\xe9\x9b\x86\xe4\xb8\xad\xe4\xba\x8e\xe5\xbe\x90\xe5\xb7\x9e\xef\xbc\x8c\xe5\x8d\x8e\xe5\x8c\x97\xe5\x8d\xa0\xe9\xa2\x86\xe5\x9c\xb0\xe5\xb0\xb1\xe5\x87\xba\xe4\xba\x86\xe5\xa4\xa7\xe7\xa9\xba\xe9\x9a\x99\xef\xbc\x8c\xe7\xbb\x99\xe4\xba\x88\xe6\xb8\xb8\xe5\x87\xbb\xe6\x88\x98\xe4\xba\x89\xe4\xbb\xa5\xe6\x94\xbe\xe6\x89\x8b\xe5\x8f\x91\xe5\xb1\x95\xe7\x9a\x84\xe6\x9c\xba\xe4\xbc\x9a\xe3\x80\x82\xe4\xbb\xa5\xe4\xb8\x8a\xe6\x98\xaf\xe6\x95\x8c\xe4\xba\xba\xe8\x87\xaa\xe5\xb7\xb1\xe5\xbc\x84\xe9\x94\x99\xef\xbc\x8c\xe4\xb8\x8d\xe6\x98\xaf\xe6\x88\x91\xe4\xbb\xac\xe4\xbd\xbf\xe4\xb9\x8b\xe9\x94\x99\xe7\x9a\x84\xe3\x80\x82\xe6\x88\x91\xe4\xbb\xac\xe6\x96\xb9\xe9\x9d\xa2\xef\xbc\x8c\xe5\xb0\x9a\xe5\x8f\xaf\xe6\x9c\x89\xe6\x84\x8f\xe5\x9c\xb0\xe5\x88\xb6\xe9\x80\xa0\xe6\x95\x8c\xe4\xb9\x8b\xe9\x94\x99\xe8\xaf\xaf\xef\xbc\x8c\xe5\x8d\xb3\xe7\x94\xa8\xe8\x87\xaa\xe5\xb7\xb1\xe8\x81\xaa\xe6\x98\x8e\xe8\x80\x8c\xe6\x9c\x89\xe6\x95\x88\xe7\x9a\x84\xe5\x8a\xa8\xe4\xbd\x9c\xef\xbc\x8c\xe5\x9c\xa8\xe6\x9c\x89\xe7\xbb\x84\xe7\xbb\x87\xe7\x9a\x84\xe6\xb0\x91\xe4\xbc\x97\xe6\x8e\xa9\xe6\x8a\xa4\xe4\xb9\x8b\xe4\xb8\x8b\xef\xbc\x8c\xe9\x80\xa0\xe6\x88\x90\xe6\x95\x8c\xe4\xba\xba\xe9\x94\x99\xe8\xa7\x89\xef\xbc\x8c\xe8\xb0\x83\xe5\x8a\xa8\xe6\x95\x8c\xe4\xba\xba\xe5\xb0\xb1\xe6\x88\x91\xe8\x8c\x83\xe5\x9b\xb4\xef\xbc\x8c\xe4\xbe\x8b\xe5\xa6\x82\xe5\xa3\xb0\xe4\xb8\x9c\xe5\x87\xbb\xe8\xa5\xbf\xe4\xb9\x8b\xe7\xb1\xbb\xef\xbc\x8c\xe8\xbf\x99\xe4\xbb\xb6\xe4\xba\x8b\xe7\x9a\x84\xe5\x8f\xaf\xe8\x83\xbd\xe6\x80\xa7\xe5\x89\x8d\xe9\x9d\xa2\xe5\xb7\xb2\xe7\xbb\x8f\xe8\xaf\xb4\xe8\xbf\x87\xe4\xba\x86\xe3\x80\x82\xe6\x89\x80\xe6\x9c\x89\xe8\xbf\x99\xe4\xba\x9b\xef\xbc\x8c\xe9\x83\xbd\xe8\xaf\xb4\xe6\x98\x8e\xef\xb