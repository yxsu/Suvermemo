 generation<br/>\n(global-srecode-minor-mode 1)<br/>\n;;************************************************************<br/>\n;;Set global shortcuts<br/>\n;;****************************************<br/>\n;;evernote-mode<br/>\n;(define-key global-map &quot;\\C-cec&quot; &apos;evernote-create-note-in-notebook)<br/>\n;(define-key global-map &quot;\\C-ceo&quot; &apos;evernote-open-note-in-notebook)<br/>\n;(define-key global-map &quot;\\C-ces&quot; &apos;evernote-search-node)<br/>\n;(global-set-key &quot;\\C-csS&quot; &apos;evernote-do-saved-search)<br/>\n;(define-key global-map &quot;\\C-cew&quot; &apos;evernote-write-note-in-notebook)<br/>\n;(define-key global-map &quot;\\C-cep&quot; &apos;evernote-post-region-in-notebook)<br/>\n;(define-key global-map &quot;\\C-ceb&quot; &apos;evernote-browser)<br/>\n;;cscope<br/>\n(define-key global-map [(control f3)] &apos;cscope-set-initial-directory)<br/>\n(define-key global-map [(control f4)] &apos;cscope-unset-initial-directory)<br/>\n(define-key global-map [(control f5)] &apos;cscope-find-this-symbol)<br/>\n(define-key global-map [(control f6)] &apos;cscope-find-global-definition)<br/>\n(define-key global-map [(control f7)] &apos;cscope-next-symbol)<br/>\n(define-key global-map [(control f8)] &apos;cscope-prev-symbol)<br/>\n;;****************************************<br/>\n(global-set-key [(f5)] &apos;shell)<br/>\n;;****************************************<br/>\n;set shortcut for cedet<br/>\n(defun my-cedet-hook ()<br/>\n&nbsp; (local-set-key [(control return)] &apos;semantic-ia-complete-symbol-menu)<br/>\n&nbsp; (local-set-key [(control f2)] &apos;semantic-ia-fast-jump)<br/>\n&nbsp; (local-set-key &quot;\\C-c?&quot; &apos;semantic-ia-show-doc);? for doc<br/>\n&nbsp; (local-set-key &quot;\\C-cp&quot; &apos;semantic-ia-show-summary);p for prototype<br/>\n&nbsp; (local-set-key &quot;\\C-cc&quot; &apos;semantic-ia-describe-class);c for class<br/>\n&nbsp; (local-set-key &quot;\\C-cd&quot; &apos;srecode-document-insert-comment);d for do